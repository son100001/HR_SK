    Public Sub ExecTrans(ByVal strSQL1 As String, ByVal strSQL2 As String, ByVal strSQL3 As String)
        Dim myTrans = SqlConnection1.BeginTransaction()
        Dim cmd As New SqlCommand
        cmd.Connection = SqlConnection1
        cmd.Transaction = myTrans
        Try
            cmd.CommandText = strSQL1
            cmd.ExecuteNonQuery()
            cmd.CommandText = strSQL2
            cmd.ExecuteNonQuery()
            cmd.CommandText = strSQL3
            cmd.ExecuteNonQuery()
            myTrans.Commit()

        Catch ex As Exception
            myTrans.Rollback()
        End Try

    End Sub
