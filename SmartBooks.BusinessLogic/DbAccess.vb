    Public Function Sunday_working(ByVal TimeOut As DateTime, ByVal TimeIn As DateTime) As String 'CheckOverTime
        'ExtTime - TimeOut
        Dim hh, mm, ss As Integer
        Dim str As String
        Dim Totalsecond As Int64
        Totalsecond = DateDiff(DateInterval.Second, TimeIn, TimeOut)

        hh = DivRem((Totalsecond Mod 86400), 3600, 0)
        mm = DivRem((Totalsecond Mod 3600), 60, 0)
        ss = Totalsecond Mod 60
        str = Format(hh, "00") & ":" & Format(mm, "00") & ":" & Format(ss, "00")
        Return str
    End Function
