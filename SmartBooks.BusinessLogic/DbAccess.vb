    Public Function ConvertDDMMToMMDD(ByVal day As String) As String
        If (day.Equals("")) Then
            Return ""
        Else
            Dim ngayC As String = day.Substring(0, 2)
            Dim thangC As String = day.Substring(3, 2)
            Dim namC As String = day.Substring(6, 4)
            Return thangC + "/" + ngayC + "/" + namC
        End If
    End Function

