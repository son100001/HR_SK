    Public Enum ObjectType
        StringType = 0
        IntType = 1
        DoubleType = 2
        DecimalType = 3
        DateType = 4
        BoolType = 5
    End Enum
    Public Function CheckDBNull(ByVal obj As Object, _
    Optional ByVal ObjectType As ObjectType = ObjectType.StringType) As Object
        Dim objReturn As Object
        objReturn = obj
        If ObjectType = ObjectType.StringType And IsDBNull(obj) Then
            objReturn = ""
        ElseIf ObjectType = ObjectType.IntType And IsDBNull(obj) Then
            objReturn = 0
        ElseIf ObjectType = ObjectType.DoubleType And IsDBNull(obj) Then
            objReturn = 0.0
        ElseIf ObjectType = ObjectType.DecimalType And IsDBNull(obj) Then
            objReturn = 0.0
        ElseIf ObjectType = ObjectType.DateType And (IsDBNull(obj) Or obj.ToString.Trim = "") Then
            objReturn = New Date(1900, 1, 1)
        ElseIf ObjectType = ObjectType.BoolType And IsDBNull(obj) Then
            objReturn = False
        End If
        Return objReturn
    End Function

    Public Function CheckNullValue(ByVal obj As String, _
    Optional ByVal ObjectType As ObjectType = ObjectType.StringType) As Object
        Dim objReturn As Object
        objReturn = obj
        If ObjectType = ObjectType.StringType Then
            If (obj Is Nothing) Then
                objReturn = ""
            End If
        ElseIf ObjectType = ObjectType.IntType Then
            Try
                If (obj Is Nothing) Then
                    objReturn = 0
                Else
                    objReturn = Integer.Parse(obj)
                End If
            Catch ex As Exception
                objReturn = 0
            End Try
        ElseIf ObjectType = ObjectType.DoubleType Then
            Try
                If (obj Is Nothing) Then
                    objReturn = 0.0
                Else
                    objReturn = Double.Parse(obj)
                End If
            Catch ex As Exception
                objReturn = 0.0
            End Try
        ElseIf ObjectType = ObjectType.DecimalType Then
            Try
                If (obj Is Nothing) Then
                    objReturn = 0.0
                Else
                    objReturn = Decimal.Parse(obj)
                End If
            Catch ex As Exception
                objReturn = 0.0
            End Try
        ElseIf ObjectType = ObjectType.DateType Then
            If obj Is Nothing Or obj = "" Then
                objReturn = New Date(1900, 1, 1)
            Else
                ' chia chuoi ra lay d/M/Y
                Dim _str As String() = obj.Split("/"c)
                If _str.Length = 3 Then
                    objReturn = StringToDate(obj)
                Else
                    objReturn = New Date(1900, 1, 1)
                End If
            End If
        ElseIf ObjectType = ObjectType.BoolType Then
            If obj = "" Or obj = "0" Or obj = "false" Then
                objReturn = False
            Else
                objReturn = True
            End If
        End If
        Return objReturn
    End Function
    Public Function StringToDate(ByVal strddMMyyyy As String) As Date
        Try
            If (strddMMyyyy.Trim() = String.Empty Or strddMMyyyy.Trim() = "") Then Return New Date(1900, 1, 1)
            Dim dd As Integer
            Dim MM As Integer
            Dim yyyy As Integer
            Dim datadate() As String
            datadate = strddMMyyyy.Split("/")
            dd = CType(datadate(0), Integer) 'Microsoft.VisualBasic.Left(strddMMyyyy, 2)
            MM = CType(datadate(1), Integer) 'strddMMyyyy.Substring(3, 2)
            yyyy = CType(datadate(2), Integer) 'Microsoft.VisualBasic.Right(strddMMyyyy, 4)
            Return New Date(yyyy, MM, dd)
        Catch ex As Exception
            Return New Date(1900, 1, 1)
        End Try
    End Function


