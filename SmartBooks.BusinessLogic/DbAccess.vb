    Public Function checkNumber(ByVal inputstring As String) As Boolean
        Dim Numeric As String

        If inputstring.Split(".").Length > 1 Then
            Return True
        End If

        Numeric = "^(-|)+([0-9]{1,})$"
        If Regex.IsMatch(inputstring, Numeric) = True Then
            Return True
        Else
            Return False
        End If
    End Function
